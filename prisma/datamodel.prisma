type User {
  id: ID! @id
  email: String @unique # Meetup doesn't provide email. We need to provide user another way to do this
  meetup_id: String! @unique # Using this to find uniques since email is not available
  access_token: String!
  refresh_token: String!
  groups: [Group]
}

type Group {
  id: ID! @id # Same as meetup
}

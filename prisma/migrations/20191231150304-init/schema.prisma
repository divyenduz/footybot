datasource ds {
  provider = "mysql"
  url = "***"
}

generator photon {
  provider = "photonjs"
}

model User {
  id            String  @default(cuid()) @id
  email         String? @unique
  meetup_id     String  @unique
  access_token  String
  refresh_token String
  groups        Group[]
}

model Group {
  id String @default(cuid()) @id
}